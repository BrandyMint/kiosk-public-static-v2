{
  "name": "KioskPublicStatic",
  "version": "0.0.71",
  "description": "JS application for kormilica.info",
  "repository": {
    "type": "git",
    "url": "git@github.com:BrandyMint/kiosk-public-static-v2.git"
  },
  "scripts": {
    "build": "npm run build:widgets && npm run build:snippet",
    "build:snippet": "WIDGETS_URL=/scripts/widgets envify app/scripts/embeddable/snippet.js | uglifyjs -m -c unused,booleans,conditionals,dead_code,if_return,join_vars > build/scripts/snippet.js",
    "build:widgets": "webpack --config webpack.config.development.js",
    "dist": "gulp dist && npm run dist:widgets && npm run dist:snippet && npm run dist:test",
    "dist:snippet": "WIDGETS_URL=http://assets.kiiiosk.ru/widgets envify app/scripts/embeddable/snippet.js | uglifyjs -m -c unused,booleans,conditionals,dead_code,if_return,join_vars > dist/scripts/snippet.js",
    "dist:test": "node ./dist_tests/prerender",
    "dist:widgets": "webpack --config webpack.config.production.js",
    "deploy": "gulp deploy",
    "start": "gulp server"
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "cookies-js": "^1.2.2",
    "deep-diff": "^0.3.2",
    "flux": "^2.0.1",
    "immutable": "^3.7.4",
    "jquery": "^2.1.4",
    "jss": "^2.3.5",
    "jss-nested": "^0.2.0",
    "lodash": "^3.8.0",
    "nouislider": "leongersen/noUiSlider#d207954f",
    "numeral": "^1.5.3",
    "perfect-scrollbar": "^0.6.2",
    "react": "^0.13.3",
    "redux": "^0.12.0",
    "reqwest": "^1.1.6",
    "store": "^1.3.17",
    "tinycolor2": "^1.1.2",
    "urijs": "^1.17.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-loader": "^5.3.2",
    "babelify": "^6.1.2",
    "bower": "^1.4.1",
    "browser-sync": "^2.7.12",
    "browserify": "^10.1.3",
    "chai": "^3.2.0",
    "coffee-reactify": "^2.4.1",
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-bump": "^1.0.0",
    "gulp-file-include": "^0.11.0",
    "gulp-flatten": "0.0.4",
    "gulp-gh-pages": "^0.5.2",
    "gulp-haml-coffee": "0.0.6",
    "gulp-minify-css": "^0.3.11",
    "gulp-mocha-phantomjs": "^0.8.1",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "1.1.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "i18next-client": "^1.10.3",
    "mocha": "^2.2.5",
    "pretty-hrtime": "^0.2.2",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.1.1",
    "semver": "^5.0.3",
    "sinon": "^1.16.1",
    "uglifyjs": "^2.4.10",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.3",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
